{
  "name": "front-end-dojo",
  "version": "0.0.5",
  "description": "Library full of useful CSS, Sass, Styled-components, JavaScript, React functions, mixins, utilities, etc.",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/alexdevero/front-end-dojo.git"
  },
  "homepage": "https://github.com/alexdevero/front-end-dojo#readme",
  "bugs": {
    "url": "https://github.com/alexdevero/front-end-dojo/issues"
  },
  "author": {
    "name": "Alex Devero",
    "email": "deveroalex@gmail.com",
    "url": "https://github.com/alexdevero"
  },
  "keywords": [
    "css",
    "functions",
    "hacks",
    "helpers",
    "javascript",
    "mixins",
    "react",
    "sass",
    "styled",
    "styled-components",
    "utilities"
  ],
  "engines": {
    "node": ">=8.x",
    "npm": ">=5.x",
    "yarn": ">=1.x.x"
  },
  "scripts": {
    "test": "set NODE_ENV=test && npm run test:css && npm run test:js && npm run test:sass",
    "test:css": "set NODE_ENV=test && stylelint ./src/css/*.css",
    "test:js": "set NODE_ENV=test && eslint ./src/javascript/*.js",
    "test:sass": "set NODE_ENV=test && sass-lint -v -q ./src/sass/*.scss"
  },
  "devDependencies": {
    "eslint": "^4.12.0",
    "sass-lint": "^1.12.1",
    "stylelint": "^8.3.1"
  }
}
